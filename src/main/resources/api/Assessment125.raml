          application/json:
            example:
              message: employee added
  put:
    body:
      application/json:
        type: employeeData
        example: !include EmployeeExamples//postexample.raml
    responses:
      200:
        body:
          application/json:
            example:
              message: employee updated 
  
                    
              


                  
          
          
                      
            
          
          
        
      
