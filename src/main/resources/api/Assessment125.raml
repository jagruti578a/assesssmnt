types:
  employeeData: !include EmployeeDatatypes/dataType.raml
/Employee:
  get:
    
    responses:
      200:
        body:
          application/json:
            type: employeeData[]
            example: !include EmployeeExamples/example.raml
                        
        
  post:

    body:
      application/json:
        type: employeeData
        example: !include EmployeeExamples/postexample.raml
